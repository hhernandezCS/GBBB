	<parameter name="fecha_inicial" class="java.util.Date"/>
	<parameter name="fecha_final" class="java.util.Date"/>
	<queryString>
		<![CDATA[select ld.* ,
(case when sl.estado_seguimiento = 0 then 'Efectivo' when sl.estado_seguimiento = 1 then 'No efectivo' when sl.estado_seguimiento = 2 then 'Descartado' when sl.estado_seguimiento = 3 then 'No interesado' else 'Otro' end )as estado_seguimiento,
(case when sl.tipo_contacto = 1 then 'Llamada' when sl.tipo_contacto = 2 then 'Mensaje' when sl.tipo_contacto = 3 then 'Correo' else 'Otro' end)as tipo_contacto,
(case when sl.valoracion_contacto = 1 then 'Ocupado' when sl.valoracion_contacto = 2 then 'Interesado' when sl.valoracion_contacto = 3 then 'Muy interesado' when sl.valoracion_contacto = 4 then 'No interesado' else 'Otro' end)as valoracion_contacto,
(case when sl.idioma_llamada = 1 then 'Español' when sl.idioma_llamada = 2 then 'Ingles' else 'Otro' end)as idioma_llamada,
(case when sl.estado_comunicacion = 1 then 'Efectivo' when sl.estado_comunicacion = 2 then 'No efectivo' else 'Otro'end)as estado_comunicacion,
(cll.descripcion)as clasificacion_llamada, (sl.f_creacion)as f_creacion_actividad, sl.f_actividad
from crm.leads ld
	inner join crm.seguimientos_leads sl
		on sl.id_lead = ld.id_lead
	inner join crm.clasificacion_llamadas cll
		on cll.id_clasificacion_llamada = sl.id_clasificacion_llamada
where ld.estado = 2 and  sl.estado = 1 and ld.id_empresa = $P{id_empresa} and sl.f_actividad between $P{fecha_inicial}  and $P{fecha_final}
group by ld.u_responsable, ld.id_lead, sl.estado_seguimiento, sl.tipo_contacto, sl.valoracion_contacto, sl.idioma_llamada, sl.estado_comunicacion, cll.descripcion, sl.f_creacion, sl.f_actividad
order by sl.f_actividad asc]]>
	</queryString>
